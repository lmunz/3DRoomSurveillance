             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NavigationDrawerPopUpMenu2"
             mc:Ignorable="d" 
             Background="Aquamarine">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="11*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        <Image Name="displayImg" HorizontalAlignment="Left" VerticalAlignment="Top" Width="720" Height="360"/>
        <Button Name="btnDepthmap" Click="BtnDepthmap_Click" Background="#1abc9c" Content="Tiefenansicht" HorizontalAlignment="Right" Margin="9,14,20,20" VerticalAlignment="Bottom" Width="200"/>
    </Grid>
</UserControl>
